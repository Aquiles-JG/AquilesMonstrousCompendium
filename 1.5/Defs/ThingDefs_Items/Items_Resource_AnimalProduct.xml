<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!--=============== Eggs ================-->
  <ThingDef ParentName="EggUnfertBase">
    <defName>Aq_StirgeEggUnfertilized</defName>
    <label>stirge egg (unfert.)</label>
    <description>The sticky egg of a stirge. It can be eaten raw, but it's much, much better cooked.</description>
    <graphicData>
      <texPath>Things/Item/InsectEgg</texPath>
      <drawSize>0.8</drawSize>
    </graphicData>
    <statBases>
      <MarketValue>6</MarketValue>
    </statBases>
  </ThingDef>
  
  <ThingDef ParentName="EggFertBase">
    <defName>Aq_StirgeEggFertilized</defName>
    <label>stirge egg (fert.)</label>
    <description>A fertilized stirge egg. If all goes well, it should hatch into a stirge larvling. It can be eaten raw, but it's much better cooked.</description>
    <possessionCount>1</possessionCount>
    <graphicData>
      <texPath>Things/Item/InsectEgg</texPath>
      <color>(245,245,245)</color>
      <drawSize>0.8</drawSize>
    </graphicData>
    <statBases>
      <MarketValue>6</MarketValue>
    </statBases>
    <thingSetMakerTags>
		<li>NotForGuests</li>
	</thingSetMakerTags>
    <comps>
      <li Class="CompProperties_Hatcher">
        <hatcherDaystoHatch>5</hatcherDaystoHatch>
        <hatcherPawn>Aq_Stirge</hatcherPawn>
      </li>
    </comps>
  </ThingDef>

</Defs>
